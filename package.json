{
  "scripts": {
    "prettier": "prettier . --check",
    "prettier:fix": "prettier . --write",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "lint": "npm run prettier && npm run  eslint",
    "lint:fix": "failed=0; npm run prettier:fix || failed=1; npm run  eslint:fix || failed=1; exit $failed;"
  },
  "devDependencies": {
    "@eslint/js": "^9.7.0",
    "@stylistic/eslint-plugin-js": "^2.6.0",
    "@types/eslint__js": "^8.42.3",
    "eslint": "^9.7.0",
    "globals": "^15.8.0",
    "prettier": "3.3.3"
  }
}
